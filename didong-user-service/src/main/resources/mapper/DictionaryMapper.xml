<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.didong.mapper.DictionaryMapper">
    <sql id="Base_Column_List">
        id, operator, opt_group, opt_code, opt_value,opt_desc,del_flag,create_time,last_update_time
    </sql>
    <select id="selectDictionary" parameterType="com.didong.serviceEntity.Dictionary" resultType="com.didong.serviceEntity.Dictionary">
      select
      <include refid="Base_Column_List"/>
      from tb_dictionary
    </select>

</mapper>